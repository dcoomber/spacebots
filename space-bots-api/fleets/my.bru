meta {
  name: Returns the list of your fleets
  type: http
  seq: 4
}

get {
  url: {{baseUrl}}/v1/fleets/my
  body: none
  auth: none
}

auth:bearer {
  token: 
}

assert {
  res.status: eq 200
}

script:post-response {
  let fleetInstance = bru.getEnvVar("fleetInstance");
  let data = res.getBody();
  bru.setVar("currentFleetId", data[fleetInstance].id);
  bru.setVar("currentSystemId", data[fleetInstance].locationSystemId);
}
